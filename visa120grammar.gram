#JSGF V1.0;

grammar visa120grammar;

public <phrases> = (<create> | <paint>);

<create> = create (<object> | <animal>);
<object> = (cloud | couch | pillow | water | tree | lamp | fishbowl | earth | star | planet | table | frame | television);
<animal> = (cat | dog | hamster | mouse | horse | shark | fish);

<paint> = paint <color>;
<color> = (red | green | blue | yellow | cyan | magenta | brown | black | white);