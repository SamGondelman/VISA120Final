#JSGF V1.0;

grammar visa120grammar;

public <phrases> = (<create> | <paint>);

<create> = create (<object> | <animal>);
<object> = (couch | pillow | water | tree);
<animal> = (cat | dog | hamster | mouse | horse);

<paint> = paint <color>;
<color> = (red | green | blue | yellow | cyan | magenta | black | white);